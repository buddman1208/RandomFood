<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="moe.kotohana.randomfood">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NearFoodActivity" />
        <activity android:name=".NearFoodMapActivity" />
        <activity android:name=".UserSettingsActivity" />
        <activity android:name=".FoodStackActivity" />
        <activity android:name=".FoodStackSearchActivity"></activity>
    </application>

</manifest>